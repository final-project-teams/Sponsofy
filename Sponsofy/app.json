{
  "expo": {
    "name": "Sponsofy",
    "slug": "Sponsofy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "extra": {
      "API_URL": "http://192.168.159.201:3000/api",
      "SOCKET_URL": "http://192.168.159.201:3000"
    },
    
    "fonts": [
      {
        "Poppins-Regular": "./assets/fonts/Poppins-Regular.ttf"
      },
      {
        "Poppins-Medium": "./assets/fonts/Poppins-Medium.ttf"
      },
      {
        "Poppins-SemiBold": "./assets/fonts/Poppins-SemiBold.ttf"
      },
      {
        "Poppins-Bold": "./assets/fonts/Poppins-Bold.ttf"
      }
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.sponsofy",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.yourcompany.sponsofy",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
